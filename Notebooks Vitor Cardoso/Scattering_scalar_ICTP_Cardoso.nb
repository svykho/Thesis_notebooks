Cell[BoxData[{
 RowBox[{
  RowBox[{"M", "=", "1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "=", 
    RowBox[{"1", "-", 
     RowBox[{"2", 
      RowBox[{"M", "/", "r"}]}]}]}], ";", 
   RowBox[{"f1", "=", 
    RowBox[{"D", "[", 
     RowBox[{"f", ",", "r"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"define", " ", "tortoise", " ", "coordinate"}], "*)"}], 
  RowBox[{
   RowBox[{"rt", "=", 
    RowBox[{"Integrate", "[", 
     RowBox[{
      RowBox[{"1", "/", "f"}], ",", "r"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"l", "=", "1"}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Initial", " ", "data", " ", "I"}], ",", " ", 
    RowBox[{
    "which", " ", "I", " ", "consider", " ", "to", " ", "be", " ", "only", 
     " ", "psidot"}]}], "*)"}], 
  RowBox[{
   RowBox[{"Initdata", "=", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"r", "+", 
          RowBox[{"2", " ", 
           RowBox[{"Log", "[", 
            RowBox[{
             RowBox[{"-", "2"}], "+", "r"}], "]"}]}], "-", "r0"}], ")"}], "^",
         "2"}]}], "/", 
      RowBox[{"sigma", "^", "2"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"r0", " ", "is", " ", "the", " ", "location"}], ",", " ", 
    RowBox[{"in", " ", "tortoise", " ", "coordinates"}], ",", " ", 
    RowBox[{
    "of", " ", "the", " ", "peak", " ", "of", " ", "the", " ", "Gaussian", 
     " ", "initial", " ", "data"}]}], "*)"}], 
  RowBox[{
   RowBox[{"r0", "=", "10"}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "sigma", " ", "is", " ", "the", " ", "width", " ", "of", " ", "the", " ", 
    "Gaussian", " ", "wavepacket"}], "*)"}], 
  RowBox[{
   RowBox[{"sigma", "=", "6"}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"r1", "=", 
    RowBox[{"2", "*", 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"10", "^", 
        RowBox[{"(", 
         RowBox[{"-", "4"}], ")"}]}]}], ")"}]}]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"[", 
     RowBox[{"r1", ",", "r2"}], "]"}], " ", "define", " ", "the", " ", 
    "integration", " ", "interval", " ", "over", " ", "the", " ", "source", 
    " ", "and", " ", "avoids", " ", "integrating", " ", "over", " ", "too", 
    " ", "large", " ", "and", " ", "unnecessary", " ", "a", " ", 
    RowBox[{"domain", ":"}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rsource1", "=", "r1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"rsource2", "=", 
    RowBox[{"r0", "+", 
     RowBox[{"4", "*", "sigma"}]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ii", "=", "0"}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Nmaxx", "=", "600"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"While", "[", 
  RowBox[{
   RowBox[{"ii", "<", 
    RowBox[{"Nmaxx", "+", "1"}]}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"w", "=", 
     RowBox[{"0.002", "+", 
      RowBox[{"2", "*", 
       RowBox[{"10", "^", 
        RowBox[{"(", 
         RowBox[{"-", "3"}], ")"}]}], "*", "ii"}]}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{
      "Numerical", " ", "infinity", " ", "is", " ", "chosen", " ", "such", 

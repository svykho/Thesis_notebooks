        " ", "a", " ", "large", " ", "number", " ", "of", " ", 
       "wavelengths"}], ",", " ", 
      RowBox[{
      "thus", " ", "ensuring", " ", "we", " ", "are", " ", "in", " ", "the", 
       " ", "wave", " ", "zone"}]}], "*)"}], 
    RowBox[{"r2", "=", 
     RowBox[{"100", "/", "w"}]}], ";", "\[IndentingNewLine]", "\n", 
    RowBox[{"ahor", "=", 
     FractionBox[
      RowBox[{"\[ImaginaryI]", " ", 
       RowBox[{"(", 
        RowBox[{"1", "+", "l", "+", 
         SuperscriptBox["l", "2"]}], ")"}]}], 
      RowBox[{"2", " ", "M", " ", 
       RowBox[{"(", 
        RowBox[{"\[ImaginaryI]", "+", 
         RowBox[{"4", " ", "M", " ", "w"}]}], ")"}]}]]}], ";", 
    RowBox[{"bhor", "=", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{
        RowBox[{"2", " ", 
         SuperscriptBox["l", "3"]}], "+", 
        SuperscriptBox["l", "4"], "+", 
        RowBox[{"12", " ", "\[ImaginaryI]", " ", "M", " ", "w"}], "+", 
        RowBox[{"l", " ", 
         RowBox[{"(", 
          RowBox[{"2", "+", 
           RowBox[{"8", " ", "\[ImaginaryI]", " ", "M", " ", "w"}]}], ")"}]}],
         "+", 
        RowBox[{
         SuperscriptBox["l", "2"], " ", 
         RowBox[{"(", 
          RowBox[{"3", "+", 
           RowBox[{"8", " ", "\[ImaginaryI]", " ", "M", " ", "w"}]}], 
          ")"}]}]}], 
       RowBox[{"16", " ", 
        SuperscriptBox["M", "2"], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", 
          RowBox[{"6", " ", "\[ImaginaryI]", " ", "M", " ", "w"}], "+", 
          RowBox[{"8", " ", 
           SuperscriptBox["M", "2"], " ", 
           SuperscriptBox["w", "2"]}]}], ")"}]}]]}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "Expanded", " ", "field", " ", "equation", " ", "close", " ", "to", " ", 
      "the", " ", "horizon", " ", "to", " ", "get", " ", "more", " ", 
      "accurate", " ", "behavior"}], "*)"}], 
    RowBox[{"Hor", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"ahor", "*", 
         RowBox[{"(", 
          RowBox[{"r", "-", 
           RowBox[{"2", "*", "M"}]}], ")"}]}], "+", 
        RowBox[{"bhor", "*", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"r", "-", 
            RowBox[{"2", "*", "M"}]}], ")"}], "^", "2"}]}]}], ")"}], "*", 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "I"}], "*", "w", "*", "rt"}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"BC", "=", 
     RowBox[{"Hor", "/.", 
      RowBox[{"r", "\[Rule]", "r1"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"BC1", "=", 
     RowBox[{
      RowBox[{"D", "[", 
       RowBox[{"Hor", ",", "r"}], "]"}], "/.", 
      RowBox[{"r", "\[Rule]", "r1"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"EQ", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"f", "^", "2"}], "*", 
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"y", "[", "r", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"r", ",", "2"}], "}"}]}], "]"}]}], "+", 
      RowBox[{"f", "*", "f1", "*", 
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"y", "[", "r", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"r", ",", "1"}], "}"}]}], "]"}]}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"w", "^", "2"}], "-", 
         RowBox[{"f", "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"l", "*", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"l", "+", "1"}], ")"}], "/", 
              RowBox[{"r", "^", "2"}]}]}], "+", 
            RowBox[{"f1", "/", "r"}]}], ")"}]}]}], ")"}], "*", 
       RowBox[{"y", "[", "r", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"sol", "=", 
     RowBox[{"NDSolve", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"EQ", "==", "0"}], ",", 
         RowBox[{
          RowBox[{"y", "[", "r1", "]"}], "\[Equal]", "BC"}], ",", 
         RowBox[{
          RowBox[{
           SuperscriptBox["y", "\[Prime]",
            MultilineFunction->None], "[", "r1", "]"}], "\[Equal]", "BC1"}]}],
         "}"}], ",", "y", ",", 
       RowBox[{"{", 
        RowBox[{"r", ",", "r1", ",", "r2"}], "}"}], 
       RowBox[{"(*", 
        RowBox[{",", "\n", "            ", 
         RowBox[{"AccuracyGoal", " ", "->", " ", "18"}], ",", " ", 
         RowBox[{"PrecisionGoal", " ", "->", " ", "18"}], ",", "\n", 
         "                             ", 
         RowBox[{"WorkingPrecision", " ", "->", " ", "24"}]}], "*)"}], ",", 
       RowBox[{"MaxSteps", "->", "340000"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"ain", "=", 
     RowBox[{"-", 
      FractionBox[
